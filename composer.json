{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
          "type": "vcs",
          "url":  "git@github.com:SaintPeter/JpGraph.git",
          "vendor-alias": "jpgraph"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "bensampo/laravel-enum": "^1.4",
        "davejamesmiller/laravel-breadcrumbs": "^5.3.2",
        "fideloper/proxy": "^4.0",
        "jpgraph/jpgraph": "4.2.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.7.*",
        "parsecsv/php-parsecsv": "0.4.5",
        "predis/predis": "^1.1",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2.9",
        "barryvdh/laravel-ide-helper": "^2.6.7",
        "doctrine/dbal": "~2.3",
        "filp/whoops": "~2.7.1",
        "fzaninotto/faker": "~1.4",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Classes",
            "app/Models",
            "app/Console/Commands"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
      "update-models": [
        "php artisan ide-helper:models -W"
      ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
